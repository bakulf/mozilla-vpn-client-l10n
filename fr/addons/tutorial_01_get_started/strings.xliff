      <note annotates="source" from="developer">A tutorial step tooltip</note>
        <target>Sélectionnez votre emplacement</target>
        <source>Select your location</source>
      </trans-unit>
      <trans-unit id="tutorial.01_get_started.step.toggle" xml:space="preserve">
      <note annotates="source" from="developer">A tutorial step tooltip</note>
        <target>Activer ou désactiver le VPN</target>
        <source>Toggle this switch to activate or deactivate the VPN</source>
      </trans-unit>
