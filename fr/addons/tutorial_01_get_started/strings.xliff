      <trans-unit id="tutorial.01_get_started.title" xml:space="preserve">
      <note annotates="source" from="developer">Title for a tutorial view</note>
        <target>Premiers pas avec le VPN</target>
        <source>Getting started with VPN</source>
      </trans-unit>
