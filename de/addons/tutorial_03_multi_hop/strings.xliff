      <trans-unit id="tutorial.03_multi_hop.step.s1" xml:space="preserve">
        <note annotates="source" from="developer">A tutorial step tooltip</note>
        <target>Auswählen, um die Server-Standorte festzulegen</target>
        <source>Select to choose server locations</source>
      </trans-unit>
      <trans-unit id="tutorial.03_multi_hop.step.s2" xml:space="preserve">
        <note annotates="source" from="developer">A tutorial step tooltip</note>
        <target>Auswählen, um „Multi-Hop“ zu aktivieren</target>
        <source>Select “Multi-hop” to activate</source>
      </trans-unit>
      <trans-unit id="tutorial.03_multi_hop.step.s3" xml:space="preserve">
